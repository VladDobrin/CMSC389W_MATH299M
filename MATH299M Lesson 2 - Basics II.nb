   RowBox[{"MyFold", "[", 
    RowBox[{"f_", ",", "acc_", ",", 
     RowBox[{"{", "}"}]}], "]"}], ":=", "acc"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
