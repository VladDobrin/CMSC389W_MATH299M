Cell[TextData[{
 StyleBox["Subscripts and superscripts\n",
  FontWeight->"Bold"],
 "Use Ctrl+^ for superscript, Ctrl+_ for subscript, Ctrl+% for subscript when \
there\[CloseCurlyQuote]s already a superscript and Ctrl+& for hat and Ctrl+$ \
for below\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["y", "3"], TraditionalForm]],ExpressionUUID->
  "6938b42f-3527-4476-aa95-d33903c3efbb"],
 "\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]],ExpressionUUID->
  "2695df89-663a-4282-b6d2-09cf357d1df6"],
 "\n",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "0"], TraditionalForm]],ExpressionUUID->
  "80de76e8-7178-4a63-9afa-653b47357938"],
 "\n",
 Cell[BoxData[
  FormBox[
   OverscriptBox["x", "^"], TraditionalForm]],ExpressionUUID->
  "f7e15fc8-ae01-4f36-8c6a-0cf80cde4b96"],
 "\n",
 Cell[BoxData[
  FormBox[
   UnderscriptBox["m", "_"], TraditionalForm]],ExpressionUUID->
  "2f6bcc85-1374-4c4b-8e3d-9fbc316570eb"],
 "\n",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["e", 
    SuperscriptBox["i\[Theta]", "2"]], TraditionalForm]],ExpressionUUID->
  "fb4aa3f3-cbe3-4b94-a7ae-8cdd03e5add9"],
 "\n",
